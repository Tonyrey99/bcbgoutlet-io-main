  },

  "slider-layout#demo-images": {
    "props": {"autoplay": {
        "timeout": 5000
      },

      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "showPaginationDots": "always",
      "blockClass": "carousel"
    }
  },

  "slider-layout#demo-products": {
    "props": {
      "autoplay": {
        "timeout": 5000
      },
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "mobileOnly",
      "blockClass": "shelf"
    }
  }

}
