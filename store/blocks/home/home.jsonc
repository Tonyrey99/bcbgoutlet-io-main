{
  "store.home": {
    "blocks": [
      "flex-layout.row#main-banners",
      "flex-layout.row#deals",
      "flex-layout.row#home-shelf-01",
      "flex-layout.row#home-shelf-02",
      "flex-layout.row#home-content-banner-01", 
      "flex-layout.row#mini-banners",
      "flex-layout.row#home-content-banner-02",
      //"flex-layout.row#home-shelf-03",
      //"flex-layout.row#mini-banners_2",
      "flex-layout.row#newsletter"
